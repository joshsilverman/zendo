<% form_for :dump, :url=>{:controller=>"documents", :action=>"create_from_csv"}, :html => { :multipart => true } do |f| -%>
 <table>
   <tr>
     <td>
      <label for="dump_file">
        Select a CSV File :
      </label>
     </td>
     <td >
       <%= f.file_field :file %>
     </td>
   </tr>
   <tr>
     <td>
       <label for="name">
         Name your document
       </label>
     </td>
     <td>
       <%=  f.text_field :name%>
     </td>
   </tr>
   <tr>
     <td colspan='2'>
       <%= submit_tag 'Upload' %>
     </td>
   </tr>
 </table>
<% end %>