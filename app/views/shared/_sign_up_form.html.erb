    <%= error_messages_for @resource %>

    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! rescue "" %>

      <%= f.text_field :email, :class => "field blink", :value => "Email", :title => "Email"%>
      <%= f.password_field :password, :class => "field blink", :value => "Password", :title => "Password"%>
      <%= f.submit "Sign up", :class=>"button notext" %>

    <% end %>